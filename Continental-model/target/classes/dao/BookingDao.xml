        xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
        catalog="" schema="${mainschema}" table="reservas"
        datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>idreserva</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>idreserva</Column>
    </UpdateKeys>
    <GeneratedKey>idreserva</GeneratedKey>
    <Queries>
        <Query id="current_bookings">
            <Sentence>
                <![CDATA[
          SELECT
            #COLUMNS#
          FROM
             ${mainschema}.current_books
          #WHERE#
        ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>